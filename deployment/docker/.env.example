# Environment Configuration Template
# Copy this file to .env.production or .env.development and configure

# Project Configuration
PROJECT_NAME=template
BUILD_VERSION=latest
BUILD_DATE=
BUILD_REVISION=

# Database Configuration
POSTGRES_PASSWORD=your_secure_password_here

# Security Configuration
SECRET_KEY=your_256_bit_secret_key_here
ENCRYPTION_KEY=your_256_bit_encryption_key_here
NEXUS_API_KEY=your_nexus_api_key_here

# Domain Configuration (Production)
DOMAIN=your-domain.com
ACME_EMAIL=admin@your-domain.com

# CORS Configuration
CORS_ORIGINS=*

# Monitoring Configuration
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here
TRAEFIK_BASIC_AUTH=admin:$2y$10$example_hash_here

# Development Only
REDIS_ADMIN_USER=admin
REDIS_ADMIN_PASSWORD=admin

# Optional: External Services
# OLLAMA_MODEL=llama3.2:1b
# EXTERNAL_API_KEY=your_external_api_key